{
  "name": "speccapitals-common",
  "version": "0.1.0",
  "description": "通用React组件库，从speccapitals-crm项目中提取",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.esm.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./styles": {
      "import": "./dist/styles/index.esm.js",
      "require": "./dist/styles/index.js",
      "types": "./dist/styles/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.esm.js",
      "require": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "rollup -c && pnpm build:css-vars",
    "dev": "rollup -c -w",
    "dev:all": "concurrently \"pnpm dev\" \"pnpm build:css-vars:watch\"",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepublishOnly": "pnpm build",
    "generate-component": "node scripts/generate-component.js",
    "build:css-vars": "node scripts/build-css-vars.js",
    "build:css-vars:watch": "nodemon --watch src/styles/variables.scss --exec \"pnpm build:css-vars\"",
    "release": "node scripts/publish-version.js"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-intl": ">=5.0.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react-responsive": "^9.0.2",
    "react-table-6": "^6.11.0",
    "styled-components": "^5.3.10",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-actions": "^7.0.26",
    "@storybook/addon-essentials": "^7.6.20",
    "@storybook/addon-interactions": "^7.6.20",
    "@storybook/addon-links": "^7.6.20",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.20",
    "@storybook/react": "^7.6.20",
    "@storybook/react-webpack5": "^7.6.20",
    "@storybook/test": "^7.6.20",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@types/react-modal": "^3.16.3",
    "@types/react-responsive": "^8.0.5",
    "@types/react-table": "^7.7.14",
    "@types/styled-components": "^5.1.26",
    "@types/styled-jsx": "^3.4.4",
    "babel-jest": "^29.7.0",
    "chalk": "^5.3.0",
    "concurrently": "^9.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "glob": "^11.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.4.4",
    "react-modal": "^3.16.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.4",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.89.2",
    "sass-loader": "^14.1.1",
    "storybook": "^7.6.20",
    "styled-jsx": "^5.1.2",
    "typescript": "^5.3.3",
    "yalc": "^1.0.0-pre.53"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "library"
  ],
  "author": "",
  "license": "MIT"
}
